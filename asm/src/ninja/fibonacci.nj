// Recursively calculates nth fibonacci number

Integer fib(Integer n) {
    if (n == 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    }
    else return fib(n-1) + fib(n-2);
}

void main() {
    local Integer n;
    local Integer result;

    n = readInteger();
    result = fib(n);

    writeInteger(result);
    writeCharacter('\n');
}
